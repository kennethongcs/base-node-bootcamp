<!DOCTYPE html>
<html lang="en">
  <%- include('partials/header', {title: 'Welcome'}) %>
  <body>
    <main class="container mt-4">
      <table
        class="table-hover table-bordered mt-4 text-center mx-auto"
        style="width: 700px"
      >
        <thead>
          <tr>
            <th>No.</th>
            <th>Vessel</th>
            <th>Voyage</th>
            <th>Service</th>
            <th>Port Code</th>
            <th>ETA</th>
            <th>ETD</th>
          </tr>
        </thead>
        <tbody>
          <% data.forEach((element, i) => { %>
          <tr>
            <td><%= i + 1 %></td>
            <td><%= element.vessel_name %></td>
            <td><%= element.voyage_number %></td>
            <td><%= element.service_name %></td>
            <td><%= element.port_code %></td>
            <td><%= element.eta %></td>
            <td><%= element.etd %></td>
          </tr>
          <% }) %>
        </tbody>
      </table>
    </main>
  </body>
</html>
